{
    "compilerOptions": {
      "target": "ES2018",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist", // Specify the correct output directory
      "rootDir": "./src", // Specify the correct root directory of your source files
      "resolveJsonModule": true,
      "sourceMap": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }